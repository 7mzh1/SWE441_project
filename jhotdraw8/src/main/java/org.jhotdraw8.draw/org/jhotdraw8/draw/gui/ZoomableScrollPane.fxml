<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollBar?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.SubScene?>
<GridPane fx:id="gridPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0"
          prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="org.jhotdraw8.draw.gui.ZoomableScrollPane">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="NEVER" minWidth="-Infinity"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="-Infinity" vgrow="NEVER"/>
    </rowConstraints>
    <children>
        <ScrollBar fx:id="horizontalScrollBar" GridPane.rowIndex="1"/>
        <ScrollBar fx:id="verticalScrollBar" orientation="VERTICAL" GridPane.columnIndex="1"/>
        <StackPane fx:id="viewportPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                   minHeight="0.0" minWidth="0.0">
            <children>
                <StackPane fx:id="background"/>
                <SubScene fx:id="subScene" height="200.0" width="200.0">
                    <root>
                        <Region/>
                    </root>
                </SubScene>
                <StackPane fx:id="foreground"/>
            </children>
        </StackPane>
    </children>
</GridPane>
