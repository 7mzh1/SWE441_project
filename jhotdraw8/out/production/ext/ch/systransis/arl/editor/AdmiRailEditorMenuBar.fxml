<MenuBar fx:id="menuBar" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
	<menus>
		<Menu id="file" mnemonicParsing="false" text="%file.text">
			<items>
				<MenuItem id="file.new" mnemonicParsing="false" text="New" />
				<MenuItem id="file.open" mnemonicParsing="false" text="Open" />
            <Menu id="file.openRecentMenu" mnemonicParsing="false" text="Open Recent" />
				<MenuItem id="file.save" mnemonicParsing="false" text="Save" />
            <MenuItem id="file.saveDrawing" mnemonicParsing="false" text="Save Drawing" />
				<MenuItem id="file.saveAs" mnemonicParsing="false" text="Save as..." />
            <MenuItem id="file.revert" mnemonicParsing="false" text="%file.revert.text" />
            <MenuItem id="file.export" mnemonicParsing="false" text="Export..." />
				<MenuItem id="file.print" mnemonicParsing="false" text="Print" />
				<MenuItem id="file.close" mnemonicParsing="false" text="Close" />
				<SeparatorMenuItem mnemonicParsing="false" />
				<MenuItem id="application.exit" mnemonicParsing="false" text="Exit" />
			</items>
		</Menu>
		<Menu id="edit" mnemonicParsing="false" text="%edit.text">
			<items>
				<!--<MenuItem id="edit.cut" mnemonicParsing="false" text="Cut"/>-->
				<!--<MenuItem id="edit.copy" mnemonicParsing="false" text="Copy"/>-->
				<!--<MenuItem id="edit.paste" mnemonicParsing="false" text="Paste"/>-->
				<!--<MenuItem id="edit.delete" mnemonicParsing="false" text="Delete" />-->
				<!--<SeparatorMenuItem mnemonicParsing="false" />-->
				<!--<MenuItem id="edit.selectAll" mnemonicParsing="false" text="Select All"/>-->
				<MenuItem id="edit.addmileage" mnemonicParsing="false" text="Add mileage" />
				<MenuItem id="edit.addpio" mnemonicParsing="false" text="Add pio" />
            <SeparatorMenuItem mnemonicParsing="false" />
				<MenuItem id="edit.checkValid" mnemonicParsing="false" text="Check PI valid" />
				<MenuItem id="edit.autoLayoutOriginal" mnemonicParsing="false" text="Auto layout Original" />
				<MenuItem id="edit.autoLayout" mnemonicParsing="false" text="Auto layout" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem id="edit.cut" mnemonicParsing="false" text="Cut" />
            <MenuItem id="edit.copy" mnemonicParsing="false" text="Copy" />
            <MenuItem id="edit.paste" mnemonicParsing="false" text="Paste" />
            <MenuItem id="edit.delete" mnemonicParsing="false" text="Delete" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem id="edit.selectAll" mnemonicParsing="false" text="Select All" />
            <MenuItem id="edit.selectSame" mnemonicParsing="false" text="Select Same" />
				<MenuItem id="selection.extendLineMinimal" mnemonicParsing="false" text="Extend selection minimal" />
				<MenuItem id="selection.extendLine" mnemonicParsing="false" text="Extend selection" />
            <MenuItem id="edit.clearSelection" mnemonicParsing="false" text="Clear Selection" />
			</items>
		</Menu>
		<Menu id="selection" mnemonicParsing="false" text="%selection.text">
			<items>
            <MenuItem id="edit.bringToFront" mnemonicParsing="false" text="Bring to front" />
            <MenuItem id="edit.sendToBack" mnemonicParsing="false" text="Send to Back" />
				<MenuItem id="selection.resetPosition" mnemonicParsing="false" text="Reset position" />
			</items>
		</Menu>
		<Menu id="view" mnemonicParsing="false" text="%view.text">
			<items>
            <CheckMenuItem id="view.showHideLayerManager" mnemonicParsing="false" text="Layer Manager" />
				<MenuItem id="view.showHideErrorManager" mnemonicParsing="false" text="Show / Hide Errors" />
				<MenuItem id="view.showTableInspector" mnemonicParsing="false" text="Show table inspector" />
				<MenuItem id="view.showChangeLog" mnemonicParsing="false" text="Show change log" />
            <CheckMenuItem id="view.showHideInspector" mnemonicParsing="false" text="Inspector" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem id="view.dumpModel" mnemonicParsing="false" text="Dump Model" />
			</items>
		</Menu>
		<Menu id="help" mnemonicParsing="false" text="%help.text.mac">
			<items>
				<MenuItem id="application.about" mnemonicParsing="false" text="About" />
			</items>
		</Menu>
	</menus>
</MenuBar>
