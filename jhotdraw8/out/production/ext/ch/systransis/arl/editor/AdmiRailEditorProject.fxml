<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
         <top>
      		<ToolBar fx:id="toolBar" prefHeight="40.0" prefWidth="200.0" styleClass="tools" BorderPane.alignment="CENTER" />
         </top>
         <center>
            <SplitPane fx:id="vsplitPane" dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0" styleClass="flush" BorderPane.alignment="CENTER">
              <items>
                  <SplitPane fx:id="mainSplitPane" dividerPositions="0.0, 1.0">
                     <items>
                  		<ScrollPane fx:id="masterScrollPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" visible="false">
                     <content>
                     			<VBox fx:id="layerBox" />
                     </content>
                           <styleClass>
                              <String fx:value="master" />
                              <String fx:value="flush" />
                           </styleClass>
                  		</ScrollPane>
                  		<ScrollPane fx:id="viewScrollPane" fitToHeight="true" fitToWidth="true" prefHeight="400.0" prefWidth="400.0">
                           <styleClass>
                              <String fx:value="view" />
                              <String fx:value="flush" />
                           </styleClass></ScrollPane>
                  		<ScrollPane fx:id="detailsScrollPane" fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0">
                     <content>
                     			<VBox fx:id="detailsVBox" />
                     </content>
                           <styleClass>
                              <String fx:value="details" />
                              <String fx:value="flush" />
                           </styleClass>
                  		</ScrollPane>
                     </items>
                     <styleClass>
                        <String fx:value="main" />
                        <String fx:value="flush" />
                     </styleClass>
                  </SplitPane>
                  <ScrollPane fx:id="errormanagerBoxWrapper" minHeight="0.0" minWidth="0.0" prefHeight="220.0" styleClass="flush">
                     <content>
                        <VBox fx:id="errormanagerBox" prefHeight="200.0" />
                     </content>
                  </ScrollPane>
              </items>
            </SplitPane>
         </center>
      </BorderPane>
   </children>
</StackPane>
