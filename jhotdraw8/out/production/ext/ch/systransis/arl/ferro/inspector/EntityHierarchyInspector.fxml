<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.65"
            xmlns:fx="http://javafx.com/fxml/1">
    <styleClass>
        <String fx:value="inspector"/>
        <String fx:value="flush"/>
    </styleClass>
    <center>
        <TableView fx:id="tableView" editable="true" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn fx:id="typeColumn" editable="false" prefWidth="75.0" text="%object.type.text"/>
                <TableColumn fx:id="idColumn" prefWidth="75.0" text="%object.id.text"/>
                <TableColumn fx:id="attributesColumn" editable="false" prefWidth="75.0" text="%object.attributes.text"/>
                <TableColumn fx:id="errorsColumn" editable="false" prefWidth="75.0" text="%object.errors.text"/>
            </columns>
        </TableView>
    </center>
    <top>
        <GridPane hgap="8.0" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="NEVER" minWidth="-Infinity"/>
                <ColumnConstraints hgrow="SOMETIMES"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="-Infinity" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="%filter.text"/>
                <TextField fx:id="filterField" GridPane.columnIndex="1"/>
            </children>
        </GridPane>
    </top>
    <bottom>
        <Label fx:id="statusBar" BorderPane.alignment="CENTER"/>
    </bottom>
</BorderPane>
