<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.65"
      xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TextArea fx:id="textArea" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="48.0"
                  minWidth="48.0" prefColumnCount="0" prefRowCount="16" styleClass="flushTopRightLeft" wrapText="true"
                  VBox.vgrow="ALWAYS"/>
        <HBox alignment="BASELINE_LEFT">
            <children>
                <MenuButton mnemonicParsing="false" text="%options.text">
                    <items>
                        <MenuItem mnemonicParsing="false">
                            <graphic>
                                <CheckBox fx:id="updateContentsCheckBox" minWidth="24.0" mnemonicParsing="false"
                                          selected="true" text="%updateContents.checkBox.text"/>
                            </graphic>
                        </MenuItem>
                    </items>
                </MenuButton>
                <Pane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
                <Button fx:id="applyButton" mnemonicParsing="false" text="%applyButton.text">
                    <HBox.margin>
                        <Insets left="4.0"/>
                    </HBox.margin>
                </Button>
            </children>
            <VBox.margin>
                <Insets bottom="4.0" left="4.0" right="4.0" top="4.0"/>
            </VBox.margin>
        </HBox>
    </children>
    <styleClass>
        <String fx:value="inspector"/>
        <String fx:value="flush"/>
    </styleClass>
</VBox>
