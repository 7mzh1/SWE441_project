<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified">
    <!-- attribute type declarations -->
    <xs:simpleType name="DirectionEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="UP"/>
            <xs:enumeration value="DOWN"/>
            <xs:enumeration value="BOTH"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="YesNoEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="YES"/>
            <xs:enumeration value="NO"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- root element -->
    <xs:element name="TrackNetGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="BaliseGroupGroup"/>
                <xs:element ref="ConnectorGroup"/>
                <xs:element ref="EmergencyStopCodePointGroup"/>
                <xs:element ref="EndTrackGroup"/>
                <xs:element ref="EndTrackNetGroup"/>
                <xs:element ref="EngineeringInfo"/>
                <xs:element ref="GradientAreaGroup"/>
                <xs:element ref="LowerPantographAreaGroup"/>
                <xs:element ref="MainSwitchOffAreaGroup"/>
                <xs:element ref="PermanentStopGroup"/>
                <xs:element ref="PointsGroup"/>
                <xs:element ref="ReducedReversingSpeedLimitAreaGroup"/>
                <xs:element ref="ShuntingAreaGroup"/>
                <xs:element ref="SignalGroup"/>
                <xs:element ref="SpeedLimitAreaGroup"/>
                <xs:element ref="TelegramGroup"/>
                <xs:element ref="TrackNet"/>
            </xs:choice>
            <xs:attribute type="xs:string" name="schemaVersion" use="required"/>
            <xs:attribute type="xs:string" name="version" use="required"/>
        </xs:complexType>

        <!-- uniqueness requirements -->
    </xs:element>

    <!-- Children of root -->
    <xs:element name="BaliseGroupGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="BaliseGroup"/>
            </xs:choice>
            <xs:attribute type="DirectionEnum" name="direction" use="required"/>
            <xs:attribute type="xs:string" name="etcsCountryID" use="required"/>
            <xs:attribute type="xs:string" name="linkReaction1" use="required"/>
            <xs:attribute type="xs:string" name="linkReaction2" use="required"/>
            <xs:attribute type="xs:string" name="locationAccuracy" use="required"/>
            <xs:attribute type="xs:string" name="reversingStopIndicator" use="required"/>
            <xs:attribute type="xs:string" name="signedAsDuplicated" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="BufferStopGroup">
        <xs:complexType>
            <xs:attribute type="xs:string" name="distanceToEndOfAuthority" use="required"/>
            <xs:attribute type="xs:string" name="distanceToDangerPoint" use="required"/>
            <xs:attribute type="xs:string" name="dangerPointReleaseSpeed" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="ConnectorGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Connector"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="EmergencyStopCodePointGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="EmergencyStopCodePoint"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="EndTrackGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="EndTrack"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="EndTrackNetGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="EndTrackNet"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="EngineeringInfo">
        <xs:complexType>
            <xs:attribute type="xs:string" name="text" use="required"/>
            <xs:attribute type="xs:string" name="date" use="required"/>
            <xs:attribute type="xs:string" name="version" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="GradientAreaGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="GradientArea"/>
            </xs:choice>
            <xs:attribute type="DirectionEnum" name="direction" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="LowerPantographAreaGroup">
        <xs:complexType>
            <xs:attribute type="DirectionEnum" name="direction" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="MainSwitchOffAreaGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="MainSwitchOffArea"/>
            </xs:choice>
            <xs:attribute type="DirectionEnum" name="direction" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="NonStoppingAreaDueToTunnelGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="NonStoppingAreaDueToTunnel"/>
            </xs:choice>
            <xs:attribute type="DirectionEnum" name="direction" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="NonStoppingAreaDueToBridgeGroup">
        <xs:complexType>
            <xs:attribute type="DirectionEnum" name="direction" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="PointsGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Points"/>
            </xs:choice>
            <xs:attribute type="xs:string" name="maintained" use="required"/>
            <xs:attribute type="xs:integer" name="length" use="optional"/>
            <xs:attribute type="xs:integer" name="durationForMovingPoints" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="PermanentStopGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="PermanentStop"/>
            </xs:choice>
            <xs:attribute type="xs:string" name="distanceToDangerPoint" use="required"/>
            <xs:attribute type="xs:string" name="dangerPointReleaseSpeed" use="required"/>
            <xs:attribute type="xs:string" name="distanceToEndOfAuthority" use="optional"/>
            <xs:attribute type="xs:string" name="distanceToEndOfOverlap" use="optional"/>
            <xs:attribute type="xs:string" name="overlapReleaseSpeed" use="optional"/>
            <xs:attribute type="xs:string" name="overlapValidityTime" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="ReducedReversingSpeedLimitAreaGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="ReducedReversingSpeedLimitArea"/>
            </xs:choice>
            <xs:attribute type="xs:integer" name="speed" use="optional"/>
            <xs:attribute type="DirectionEnum" name="direction" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="ShuntingAreaGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="ShuntingArea"/>
            </xs:choice>
            <xs:attribute type="YesNoEnum" name="switchable" use="optional"/>
            <xs:attribute type="YesNoEnum" name="transmissionOfListOfBalisesForShuntingArea" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="SignalGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Signal"/>
            </xs:choice>
            <xs:attribute type="xs:string" name="type" use="required"/>
            <xs:attribute type="xs:string" name="maintenanceRole" use="required"/>
            <xs:attribute type="xs:string" name="distanceToDangerPoint" use="required"/>
            <xs:attribute type="xs:string" name="dangerPointReleaseSpeed" use="required"/>
            <xs:attribute type="xs:string" name="distanceToEndOfOverlap" use="required"/>
            <xs:attribute type="xs:string" name="overlapReleaseSpeed" use="required"/>
            <xs:attribute type="xs:string" name="overlapValidityTime" use="required"/>
            <xs:attribute type="xs:string" name="distanceFromOverlapTimerStartLocation" use="optional"/>
            <xs:attribute type="xs:string" name="distanceToEndOfOSSection" use="required"/>
            <xs:attribute type="xs:string" name="acknowledgementDistanceForOS" use="required"/>
            <xs:attribute type="xs:string" name="announcementDistanceForEntry" use="required"/>
            <xs:attribute type="xs:string" name="originLevels" use="required"/>
            <xs:attribute type="xs:string" name="reactToSignalStop" use="required"/>
            <xs:attribute type="xs:string" name="permanentL2Exit" use="required"/>
            <xs:attribute type="xs:string" name="safetyDistance" use="required"/>
            <xs:attribute type="xs:string" name="distanceToStartOfTAFWindowInRearOfUpgradeSignal" use="required"/>
            <xs:attribute type="xs:string" name="distanceToSignalStopLocation" use="optional"/>
            <xs:attribute type="xs:string" name="changedNVForOverlapReleaseRequired" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="SpeedLimitAreaGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="SpeedLimitArea"/>
            </xs:choice>
            <xs:attribute type="DirectionEnum" name="direction" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="TelegramGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="SignalTelegram"/>
                <xs:element ref="PointsTelegram"/>
                <xs:element ref="EmergencyStopCodePointTelegram"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="TrackSectionGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="TrackSection"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="TrackNet">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="ConnectorGroup"/>
                <xs:element ref="BaliseGroupGroup"/>
                <xs:element ref="BufferStopGroup"/>
                <xs:element ref="EmergencyStopCodePointGroup"/>
                <xs:element ref="EndTrackGroup"/>
                <xs:element ref="EndTrackNetGroup"/>
                <xs:element ref="GradientAreaGroup"/>
                <xs:element ref="LowerPantographAreaGroup"/>
                <xs:element ref="MainSwitchOffAreaGroup"/>
                <xs:element ref="NonStoppingAreaDueToTunnelGroup"/>
                <xs:element ref="NonStoppingAreaDueToBridgeGroup"/>
                <xs:element ref="PermanentStopGroup"/>
                <xs:element ref="PointsGroup"/>
                <xs:element ref="ReducedReversingSpeedLimitAreaGroup"/>
                <xs:element ref="ShuntingAreaGroup"/>
                <xs:element ref="SignalGroup"/>
                <xs:element ref="SpeedLimitAreaGroup"/>
                <xs:element ref="TelegramGroup"/>
                <xs:element ref="TrackSectionGroup"/>
                <xs:element ref="VirtualFirstMCABarrierGroup"/>
            </xs:choice>
            <xs:attribute type="xs:string" name="trackNetID" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="VirtualFirstMCABarrierGroup">
    </xs:element>
    
    <!-- Children of BaliseGroupGroup -->
    <xs:element name="BaliseGroup">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="EntryInfo"/>
                <xs:element ref="ExitInfo"/>
                <xs:element ref="NRBCInfo"/>
            </xs:choice>
            <xs:attribute type="xs:string" name="trackSegmentID" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKM" use="required"/>
            <xs:attribute type="xs:string" name="etcsCountryID" use="optional"/>
            <xs:attribute type="xs:string" name="etcsBaliseGroupID" use="required"/>
            <xs:attribute type="xs:string" name="signedAsDuplicated" use="optional"/>
            <xs:attribute type="xs:string" name="reversingStopIndicator" use="optional"/>
            <xs:attribute type="xs:integer" name="locationAccuracy" use="optional"/>
            <xs:attribute type="DirectionEnum" name="direction" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="EntryInfo">
        <xs:complexType>
            <xs:attribute type="DirectionEnum" name="direction" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="ExitInfo">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="AnnouncementByRBC"/>
            </xs:choice>
            <xs:attribute type="DirectionEnum" name="direction" use="required"/>
            <xs:attribute type="xs:string" name="signalElementIDs" use="required"/>
            <xs:attribute type="xs:string" name="exitTargetLevels" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="AnnouncementByRBC">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="BaliseGroup"/>
            </xs:choice>
            <xs:attribute type="xs:integer" name="announcementDistanceForExit" use="required"/>
            <xs:attribute type="xs:integer" name="offsetToAnnouncedTransitionPosition" use="required"/>
            <xs:attribute type="xs:integer" name="acknowledgementDistancesForExitTargetLevels" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="NRBCInfo">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="Accepting"/>
                <xs:element ref="HandingOverWithKnownSignal"/>
            </xs:choice>
            <xs:attribute type="xs:string" name="etcsCountryID" use="required"/>
            <xs:attribute type="xs:string" name="etcsRBCID" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="Accepting">
        <xs:complexType>
            <xs:attribute type="DirectionEnum" name="direction" use="required"/>
            <xs:attribute type="xs:string" name="signalElementID" use="required"/>
            <xs:attribute type="xs:integer" name="distanceFromNRBCBorderBGToRBCAnnouncedNRBCTransitionPosition" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="HandingOverWithKnownSignal">
        <xs:complexType>
            <xs:attribute type="DirectionEnum" name="direction" use="required"/>
            <xs:attribute type="xs:string" name="signalElementID" use="required"/>
            <xs:attribute type="xs:integer" name="distanceFromNRBCBorderBGToRBCAnnouncedNRBCTransitionPosition" use="required"/>
        </xs:complexType>
    </xs:element>

    <!-- Children of EmergencyStopCodePointGroup -->
    <xs:element name="EmergencyStopCodePoint">
        <xs:complexType>
            <xs:attribute type="xs:string" name="elementID" use="required"/>
            <xs:attribute type="xs:string" name="trackSegmentID" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKM" use="required"/>
            <xs:attribute type="DirectionEnum" name="direction" use="required"/>
        </xs:complexType>
    </xs:element>
    
    <!-- Children of GradientAreaGroup -->
    <xs:element name="GradientArea">
        <xs:complexType>
            <xs:attribute type="xs:string" name="trackSegmentID" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKM1" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKM2" use="required"/>
            <xs:attribute type="xs:integer" name="gradient" use="required"/>
            <xs:attribute type="DirectionEnum" name="direction" use="optional"/>
        </xs:complexType>
    </xs:element>
    
    <!-- Children of MainSwitchOffAreaGroup -->
    <xs:element name="MainSwitchOffArea">
        <xs:complexType>
            <xs:attribute type="xs:string" name="elementID" use="required"/>
            <xs:attribute type="xs:string" name="trackSegmentID" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKM1" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKM2" use="required"/>
        </xs:complexType>
    </xs:element>
    
    <!-- Children of ConnectorGroup -->
    <xs:element name="Connector">
        <xs:complexType>
            <xs:attribute type="xs:string" name="trackSegmentID1" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKM1" use="required"/>
            <xs:attribute type="xs:string" name="trackSegmentID2" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKM2" use="required"/>
        </xs:complexType>
    </xs:element>
    
    <!-- Children of EndTrackGroup -->
    <xs:element name="EndTrack">
        <xs:complexType>
            <xs:attribute type="xs:string" name="trackSegmentID" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKM" use="required"/>
            <xs:attribute type="xs:string" name="elementID" use="required"/>
        </xs:complexType>
    </xs:element>
    
    <!-- Children of EndTrackNetGroup -->
    <xs:element name="EndTrackNet">
        <xs:complexType>
            <xs:attribute type="xs:string" name="trackSegmentID" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKM" use="required"/>
            <xs:attribute type="xs:string" name="elementID" use="required"/>
        </xs:complexType>
    </xs:element>
    
    <!-- Children of NonStoppingAreaDueToTunnelGroup -->
    <xs:element name="NonStoppingAreaDueToTunnel">
        <xs:complexType>
            <xs:attribute type="xs:string" name="trackSegmentID" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKM1" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKM2" use="required"/>
        </xs:complexType>
    </xs:element>
    
    <!-- Children of PermanentStopGroup -->
    <xs:element name="PermanentStop">
        <xs:complexType>
            <xs:attribute type="xs:string" name="trackSegmentID" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKM" use="required"/>
            <xs:attribute type="DirectionEnum" name="direction" use="required"/>
        </xs:complexType>
    </xs:element>
        
    <!-- Children of PointsGroup -->
    <xs:element name="Points">
        <xs:complexType>
            <xs:attribute type="xs:string" name="elementID" use="required"/>
            <xs:attribute type="xs:string" name="trackSegmentTongue" use="required"/>
            <xs:attribute type="xs:string" name="operationalKMTongue" use="required"/>
            <xs:attribute type="xs:string" name="trackSegmentLeft" use="required"/>
            <xs:attribute type="xs:string" name="trackSegmentRight" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKMLeft" use="optional"/>
            <xs:attribute type="xs:decimal" name="operationalKMRight" use="optional"/>
            <xs:attribute type="xs:string" name="maintained" use="optional"/>
        </xs:complexType>
    </xs:element>
    
    <!-- Children of ReducedReversingSpeedLimitAreaGroup -->
    <xs:element name="ReducedReversingSpeedLimitArea">
        <xs:complexType>
            <xs:attribute type="xs:string" name="trackSegmentID" use="required"/>
            <xs:attribute type="xs:string" name="operationalKM1" use="required"/>
            <xs:attribute type="xs:string" name="operationalKM2" use="required"/>
            <xs:attribute type="DirectionEnum" name="direction" use="optional"/>
            <xs:attribute type="xs:integer" name="speed" use="optional"/>
        </xs:complexType>
    </xs:element>
    
    <!-- Children of ShuntingAreaGroup -->
    <xs:element name="ShuntingArea">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="TrackZone"/>
            </xs:choice>
            <xs:attribute type="xs:string" name="stationID" use="optional"/>
            <xs:attribute type="xs:string" name="interlockingID" use="optional"/>
            <xs:attribute type="YesNoEnum" name="switchable" use="optional"/>
            <xs:attribute type="xs:string" name="interlockingElementID" use="optional"/>
            <xs:attribute type="YesNoEnum" name="transmissionOfListOfBalisesForShuntingArea" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="TrackZone">
        <xs:complexType>
            <xs:attribute type="xs:string" name="trackSegmentID" use="required"/>
            <xs:attribute type="xs:string" name="operationalKM1" use="required"/>
            <xs:attribute type="xs:string" name="operationalKM2" use="required"/>
            <xs:attribute type="xs:string" name="trackZoneID" use="optional"/>
        </xs:complexType>
    </xs:element>
    
    
    <!-- Children of SpeedLimitAreaGroup -->
    <xs:element name="SpeedLimitArea">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element ref="TrainCategory"/>
                <xs:element ref="AxleLoad"/>
            </xs:choice>
            <xs:attribute type="xs:string" name="trackSegmentID" use="required"/>
            <xs:attribute type="xs:string" name="operationalKM1" use="required"/>
            <xs:attribute type="xs:string" name="operationalKM2" use="required"/>
            <xs:attribute type="xs:integer" name="speed" use="required"/>
            <xs:attribute type="DirectionEnum" name="direction" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="TrainCategory">
        <xs:complexType>
            <xs:attribute type="xs:integer" name="kind" use="required"/>
            <xs:attribute type="xs:integer" name="speed" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="AxleLoad">
        <xs:complexType>
            <xs:attribute type="xs:integer" name="speed" use="required"/>
            <xs:attribute type="xs:integer" name="weight" use="required"/>
        </xs:complexType>
    </xs:element>
    
    <!-- Children of SignalGroup -->
    <xs:element name="Signal">
        <xs:complexType>
            <xs:attribute type="xs:string" name="trackSegmentID" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKM" use="required"/>
            <xs:attribute type="xs:string" name="elementID" use="required"/>
            <xs:attribute type="DirectionEnum" name="direction" use="required"/>
            <xs:attribute type="xs:string" name="type" use="optional"/>
            <xs:attribute type="xs:string" name="permanentL2Exit" use="optional"/>
            <xs:attribute type="xs:string" name="safetyDistance" use="optional"/>
            <xs:attribute type="xs:string" name="distanceToStartOfTAFWindowInRearOfUpgradeSignal" use="optional"/>
            <xs:attribute type="xs:string" name="announcementDistanceForEntry" use="optional"/>
            <xs:attribute type="xs:string" name="originLevels" use="optional"/>
            <xs:attribute type="xs:string" name="changedNVForOverlapReleaseRequired" use="optional"/>
            <xs:attribute type="xs:string" name="dangerPointReleaseSpeed" use="optional"/>
            <xs:attribute type="xs:string" name="distanceToEndOfOverlap" use="optional"/>
            <xs:attribute type="xs:string" name="overlapReleaseSpeed" use="optional"/>
            <xs:attribute type="xs:string" name="overlapValidityTime" use="optional"/>
            <xs:attribute type="xs:string" name="distanceFromOverlapTimerStartLocation" use="optional"/>
            <xs:attribute type="xs:string" name="maintenanceRole" use="optional"/>
            <xs:attribute type="xs:string" name="reactToSignalStop" use="optional"/>
        </xs:complexType>
    </xs:element>
    
    <!-- Children of TrackSectionGroup -->
    <xs:element name="TrackSection">
        <xs:complexType>
            <xs:attribute type="xs:string" name="trackSegmentID" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKM1" use="required"/>
            <xs:attribute type="xs:decimal" name="operationalKM2" use="required"/>
            <xs:attribute type="xs:string" name="name" use="required"/>
        </xs:complexType>
    </xs:element>
    
    <!-- Children of TelegramGroup -->
    <xs:element name="SignalTelegram">
        <xs:complexType>
            <xs:attribute type="xs:string" name="elementID" use="required"/>
            <xs:attribute type="YesNoEnum" name="routeRequestAllowed" use="required"/>
            <xs:attribute type="xs:string" name="stationID" use="required"/>
            <xs:attribute type="xs:string" name="interlockingID" use="required"/>
            <xs:attribute type="xs:string" name="interlockingElementID" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="PointsTelegram">
        <xs:complexType>
            <xs:attribute type="xs:string" name="elementID" use="required"/>
            <xs:attribute type="xs:string" name="stationID" use="required"/>
            <xs:attribute type="xs:string" name="interlockingID" use="required"/>
            <xs:attribute type="xs:string" name="interlockingElementID" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="EmergencyStopCodePointTelegram">
        <xs:complexType>
            <xs:attribute type="xs:string" name="elementID" use="required"/>
            <xs:attribute type="xs:string" name="stationID" use="required"/>
            <xs:attribute type="xs:string" name="interlockingID" use="required"/>
            <xs:attribute type="xs:string" name="interlockingElementID" use="required"/>
        </xs:complexType>
    </xs:element>
    
</xs:schema>
