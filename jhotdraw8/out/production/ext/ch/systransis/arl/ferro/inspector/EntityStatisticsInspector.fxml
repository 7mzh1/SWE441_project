<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TextArea fx:id="textArea" editable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="48.0" minWidth="48.0" prefColumnCount="0" prefRowCount="16" styleClass="flushTopRightLeft" text="%entityStatistics.pressRefresh.text" wrapText="true" VBox.vgrow="ALWAYS" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
                <Button fx:id="refreshButton" mnemonicParsing="false" onAction="#refreshStatistics" text="%refreshButton.text" />
        </buttons>
         <VBox.margin>
            <Insets right="10.0" />
         </VBox.margin>
      </ButtonBar>
    </children>
    <styleClass>
        <String fx:value="inspector" />
        <String fx:value="flush" />
    </styleClass>
</VBox>
